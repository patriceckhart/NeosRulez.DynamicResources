{
  "description": "A package for Neos CMS which render and cache dynamic CSS (scss) and Javascript resources.",
  "type": "neos-package",
  "name": "neosrulez/dynamicresources",
  "require": {
    "neos/neos": "^7.0 || ^8.0 || ^8.1",
    "scssphp/scssphp": "^1.10",
    "matthiasmullie/minify": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "NeosRulez\\DynamicResources\\": "Classes/"
    }
  },
  "extra": {
    "neos": {
      "package-key": "NeosRulez.DynamicResources"
    }
  }
}
